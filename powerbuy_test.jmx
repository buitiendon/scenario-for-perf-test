        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="script-test-add-cart" enabled="true">
          <stringProp name="WebDriverSampler.script">var pkg = JavaImporter(org.openqa.selenium, org.openqa.selenium.support.ui,org.openqa.selenium.support.events.EventFiringWebDriver);
var wait = new pkg.WebDriverWait(WDS.browser, 150)// WebDriver wait

WDS.sampleResult.sampleStart();

// Launch url specified-01
WDS.browser.get(&apos;https://www.powerbuy.co.th/en/lg-led-tv-49-lg-uhd-smart-dtv-49sm8100pta-atm-250087&apos;); 
WDS.log.info(&quot;Launch url specified-01&quot;);

//Click add to cart-01
wait.until(pkg.ExpectedConditions.presenceOfElementLocated(pkg.By.id(&apos;btn-addCart-250087&apos;)));
var btnLogin01 = WDS.browser.findElement(pkg.By.id(&apos;btn-addCart-250087&apos;));
btnLogin01.click();

// Launch url specified-02
WDS.browser.get(&apos;https://www.powerbuy.co.th/en/dyson-tower-air-purifier-40-sqm-tp03-phth-whsv-233090&apos;); 
WDS.log.info(&quot;Launch url specified-02&quot;);

//Click add to cart-02
wait.until(pkg.ExpectedConditions.presenceOfElementLocated(pkg.By.id(&apos;btn-addCart-233090&apos;)));
var btnLogin02 = WDS.browser.findElement(pkg.By.id(&apos;btn-addCart-233090&apos;));
btnLogin02.click();

// Launch url specified-03
WDS.browser.get(&apos;https://www.powerbuy.co.th/en/apple-apple-iphone-11-256gb-black-mwm72tha-252930&apos;); 
WDS.log.info(&quot;Launch url specified-03&quot;);

//Click add to cart-03
wait.until(pkg.ExpectedConditions.presenceOfElementLocated(pkg.By.id(&apos;btn-addCart-252930&apos;)));
var btnLogin03 = WDS.browser.findElement(pkg.By.id(&apos;btn-addCart-252930&apos;));
btnLogin03.click();

WDS.sampleResult.sampleEnd();</stringProp>
